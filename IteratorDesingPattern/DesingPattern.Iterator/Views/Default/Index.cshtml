
@{
    Layout = null;
    var x = ViewBag.v;
    int count = 0;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.rtl.css" rel="stylesheet" />
<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
<div class="container">
    <br />
    <table>
        <tr>
            <th>
                #
            </th>
            <th>
                Ziyaret Noktası
            </th>
</tr>
@foreach (var item in x)
        {
            count++;
            <tr>
                <td>@count</td>
                <td>@item</td>
            </tr>
        }
    
</table>
</div>